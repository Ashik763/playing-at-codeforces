#include<bits/stdc++.h>
using namespace std;
int main(){

    long long int t,a,b,i,j;
    cin >> t ;
    while(t--){
        cin >> a >> b;
        if(a % b == 0){
            cout << 0 << endl;
        }
        else{
            j= a%b;
            cout << (b-j) << endl;
        }
    }


    return 0;
}
